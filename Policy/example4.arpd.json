{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "Federated": "arn:aws:iam::1234567890:saml-provider/sts.company.com"
      },
      "Action": "sts:AssumeRoleWithSML",
      "Condition": {
        "StirngEquals": {
          "SAML:aud": "https://signin.aws.amazon.com/saml"
        }
      }

    }
  ]
}